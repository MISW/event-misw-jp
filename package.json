{
  "scripts": {
    "build": "cp -v src/ dist/",
    "clean": "rm -rf dist/",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "sass": "sass --style compressed --no-source-map src/:src/",
    "squoosh": "squoosh-cli --webp auto src/**/*.{jpg,webp,avif,jxl,wp2,png} -d temp/"
  },
  "dependencies": {},
  "devDependencies": {
    "@squoosh/cli": "0.7.3",
    "prettier": "3.0.3",
    "sass": "1.69.1"
  }
}
