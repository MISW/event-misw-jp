{
  "name": "event-misw-jp",
  "license": "MPL-2.0",
  "scripts": {
    "build": "cp -rv src/ dist/",
    "clean": "rm -rf dist/",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "sass": "sass --style compressed --no-source-map src/:src/",
    "squoosh": "squoosh-cli --webp auto src/**/*.{jpg,webp,avif,jxl,wp2,png} -d temp/"
  },
  "dependencies": {},
  "devDependencies": {
    "@squoosh/cli": "0.7.3",
    "prettier": "3.4.2",
    "sass": "1.97.3"
  },
  "engines": {
    "node": ">=20",
    "pnpm": ">=9"
  }
}
